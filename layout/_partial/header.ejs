<header class="head-header">
  <div class="head-author">
    <a href="<%- url_for() %>" class="head-author-link">
      <%= config.title%></a>
    </div>
  <div class="head-right">
    <!-- <div class="head-search">
      <input class="head-searchK"></input>
      <span class="head-searchT">
        Search</span>
    </div> -->
    <button class="bar-wrap" id="bar-wrap-toggle">
      <span class="bar"></span>
      <span class="bar"></span>
      <span class="bar"></span>
    </button>
    <div class="head-about" id="head-about">
      <%  let otherMenu2 = config.otherMenu2 || theme.otherMenu2;
          for (var item in otherMenu2){ %>
      <a class="head-about-link" href="<%- url_for(otherMenu2[item]) %>">
        <%= item %></a>
      <% } %>
    </div>
  </div>
</header>
